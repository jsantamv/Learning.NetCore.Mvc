@model Curso

@{
    ViewData["Title"] = "Información del Curso";
    Layout = "Simple";
}

<h1>@ViewData["Title"]</h1>

<strong>Desde la vista normal</strong>
<p>
    Nombre:@Model.Nombre <br />
    Id : @Model.Id
</p>

<strong>Desde la vista parcial</strong>
<partial name="ObjetoEscuelaBase" />


<p><i>@ViewBag.Fecha</i></p>

<script src="~/js/Curso/Curso.js"></script>